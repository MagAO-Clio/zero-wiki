        &lt;td&gt;Pupil Imaging&lt;/td&gt;
         
        &lt;td&gt;f/21.5&lt;/td&gt;
         
       &lt;/tr&gt; 
       &lt;tr&gt;
         
        &lt;td&gt;Narrow Slit&lt;/td&gt;
         
        &lt;td&gt;L APP&lt;/td&gt;
         
        &lt;td&gt;3.3um&lt;/td&gt;
         
        &lt;td&gt;4% ND3&lt;/td&gt;
         
        &lt;td&gt;f/37.7&lt;/td&gt;
         
        &lt;td&gt;&lt;br class="" /&gt;&lt;/td&gt;
         
       &lt;/tr&gt; 
       &lt;tr&gt;
         
